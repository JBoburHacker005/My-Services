{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "styles.css", "use": "@vercel/static" },
    { "src": "script.js", "use": "@vercel/static" },
    { "src": "assets/**/*", "use": "@vercel/static" },
    { "src": "Airbrush-Image-Enhancer-1761540581476-Photoroom.png", "use": "@vercel/static" },
    { "src": "Sertifikat/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/styles.css", "dest": "/styles.css" },
    { "src": "/script.js", "dest": "/script.js" },
    { "src": "/Airbrush-Image-Enhancer-1761540581476-Photoroom.png", "dest": "/Airbrush-Image-Enhancer-1761540581476-Photoroom.png" },
    { "src": "/Sertifikat/(.*)", "dest": "/Sertifikat/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
